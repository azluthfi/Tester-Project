<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="@color/buttonDefaultNormalDark"
                android:endColor="@color/buttonDefaultNormalDark"
                android:angle="270" />
            <corners android:radius="2dp" />
            <stroke
                android:width="1dp"
                android:color="@color/buttonDefaultNormalDark" />
        </shape>
    </item>
    <item android:state_enabled="false" >
        <shape>
            <gradient
                android:startColor="@color/buttonDetfaultDisabled"
                android:endColor="@color/buttonDetfaultDisabled"
                android:angle="270" />
            <corners android:radius="2dp" />
            <stroke
                android:width="1dp"
                android:color="@color/buttonDetfaultDisabled" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="@color/buttonDefaultNormal"
                android:endColor="@color/buttonDefaultNormalDark"
                android:angle="270" />
            <corners android:radius="2dp" />
            <stroke
                android:width="1dp"
                android:color="@color/buttonDefaultNormal" />
        </shape>
    </item>
</selector>